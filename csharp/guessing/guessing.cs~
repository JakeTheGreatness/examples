using System;

public class Example
{
  public static void Main ()
  {
    string line;
    Console.WriteLine ("please enter some text, to finish please type ^D on an empty line");
    do { 
      line = Console.ReadLine ();
      if (line != null) 
	Console.WriteLine ("you wrote: " + line);
    } while (line != null);
    Console.WriteLine ("goodbye");
  }
}
